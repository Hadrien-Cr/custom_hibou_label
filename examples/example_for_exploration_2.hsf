@explore_option{
    strategy = BreadthFS;
    loggers = [graphic=svg];
    pre_filters = [ max_depth = 4, 
                    max_loop_depth = 2, 
                    max_node_number = 8 ]
}
@message{
    m1;
    m2
}
@lifeline{
    a;
    b
}
seq(
    loop_seq(
        b -- m1 -> a
    ),
    a -- m2 -> b
)
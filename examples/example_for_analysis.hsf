@analyze_option{
    loggers = [graphic[svg,vertical]];
    semantics = prefix;
    strategy = DepthFS;
    use_locfront = false;
    goal = Pass
}
@message{
    m1;
    m2;
    m3;
    m4
}
@lifeline{
    a;
    b;
    c
}
seq(
    loopH(
        seq(
            a -- m1 -> b,
            alt(
                b -- m2 -> c,
                o
            ),
            b -- m3 ->|
        )
    ),
    par(
        a -- m1 ->|,
        c -- m4 -> a
    )
)



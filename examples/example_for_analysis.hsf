@analyze_option{
    strategy = DFS;
    loggers = [graphic=svg];
    semantics = prefix;
    priorities = [reception=1]
}
@message{
    m1;
    m2;
    m3;
    m4
}
@lifeline{
    a;
    b;
    c
}
seq(
    loop_seq(
        seq(
            a -- m1 -> b,
            alt(
                b -- m2 -> c,
                o
            ),
            b -- m3 ->|
        )
    ),
    par(
        a -- m1 ->|,
        c -- m4 -> a
    )
)



@analyze_option{
    loggers = [graphic[svg,vertical]];
    semantics = accept;
    strategy = DepthFS;
    use_locfront = false;
    goal = Pass;
    frontier_priorities = [reception=1]
}
@message{
    m
}
@lifeline{
    a;
    b;
    c
}
par(
    loopH(
        a -- m -> b
    ),
    loopH(
        a -- m -> c
    )
)